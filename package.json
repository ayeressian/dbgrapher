{
  "name": "dbgrapher",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --mode=development",
    "start-pwa": "rm -rf ./dist && yarn build-dev && http-server ./dist -p 9997",
    "test": "karma start",
    "test-headless": "karma start --single-run --browsers ChromeHeadless",
    "e2e": "yarn e2e-chromium && yarn e2e-firefox && yarn e2e-webkit",
    "e2e-parallel": "parallel ::: \"yarn e2e-chromium\" \"yarn e2e-firefox\" \"yarn e2e-webkit\"",
    "e2e-chromium": "BROWSER_TYPE=chromium ts-mocha ./e2e/index.ts -t 20000",
    "e2e-firefox": "BROWSER_TYPE=firefox ts-mocha ./e2e/index.ts -t 20000",
    "e2e-webkit": "BROWSER_TYPE=webkit ts-mocha ./e2e/index.ts -t 20000",
    "start-and-e2e": "start-server-and-test start http://localhost:9999 e2e",
    "build-prod": "webpack --mode=production",
    "build-dev": "webpack --mode=development",
    "build-dev-watch": "webpack --mode=development --watch",
    "lint": "npx eslint . --ext .js,.ts",
    "webpack-analyse": "webpack --mode=production --profile --json > ./stats.json && webpack-bundle-analyzer ./stats.json && rm ./stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
