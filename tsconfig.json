{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "ES2019",
    "module": "CommonJS",
    "allowJs": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
  }
}